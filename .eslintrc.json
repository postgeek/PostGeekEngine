{
    "extends": "airbnb-base",
    "parser": "@babel/eslint-parser",
    "env": {
      "browser": true
    },
    "rules": {
      "no-underscore-dangle": "off",
      "no-restricted-syntax": [
        "error",
        {
          "selector": "FunctionExpression[operator='in']"
        }
      ],
      "no-underscore-dangle": "off",
      "no-plusplus": "off",
      "max-len": "off"
    }
}
